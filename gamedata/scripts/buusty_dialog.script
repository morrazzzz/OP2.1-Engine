-- Спецэффекты оружия
function weapon_effector()
	if screen.fov < 35 and xr_logic.pstor_retrieve(db.actor, "use_scope", 0) == 0 then
		local wpn
		local active_slot=db.actor:active_slot()
		if (not active_slot) or (active_slot and active_slot==-1) then
			return
		end
		wpn = db.actor:item_in_slot(active_slot)
		if wpn and wpn:section() == "wpn_crossbow_m1" or wpn and wpn:section() == "wpn_svd_m3" or wpn and wpn:section() == "wpn_b94_sil" then
			if not xr_conditions.is_day() then
				level.add_pp_effector("bast.ppe", 1034, true)
				console:execute("r2_mblur 0.6")
			end
			xr_logic.pstor_store(db.actor, "use_scope", 1)
		end

	elseif screen.fov > 35 and xr_logic.pstor_retrieve(db.actor, "use_scope", 0) == 1 then
		xr_logic.pstor_store(db.actor, "use_scope", 0)
		level.remove_pp_effector(1034)
		console:execute("r2_mblur 0.")
	end
end

-- Встроенный детектор аномалий
local det_suit_id = nil

function detector_suit()
	if db.actor:item_in_slot(6) and db.actor:item_in_slot(6):section() == "exo_doktor_outfit" then
		if not det_suit_id then
			if has("detector_suit") then
				det_suit_id = db.actor:object("detector_suit"):id()
			else
				det_suit_id = create("detector_suit",db.actor:position(), db.actor:level_vertex_id(),db.actor:game_vertex_id(), db.actor:id()).id
			end
		end
	else
		if det_suit_id and server_object(det_suit_id) then
			release(server_object(det_suit_id),true)
			det_suit_id = nil
		end
	end
end

