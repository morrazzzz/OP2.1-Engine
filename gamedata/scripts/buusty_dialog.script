		db.actor:money() >= 30000 and
		db.actor:item_in_slot(0)
end

function xo_repair_start()
	local wpn = db.actor:item_in_slot(0):section()
	
	give_info("biznes_remove_special_item")
	lost(wpn,1)
	lost_good("wpn_knife_n",1)
	lost_good("wpn_knife_new",1)
	lost_good("wpn_elf",1)
	lost("af_dummy_spring_red",1)
	lost("part_deathclaw_antler",1)
	lost_money(30000)
	disable_info("biznes_remove_special_item")
	
	amk.save_variable("xo_sec", wpn)
	amk.save_variable("xo_rep", amk.game_hours()+math.random(20, 24))
end

-- проверка примерно раз в 3 игровых минуты
function xo_repair_finished()
	if has_info("xo_repair_start") and not has_info("xo_repair_finished") and amk.game_hours() >= amk.load_variable("xo_rep", 0) then
		sms("%c[255,160,160,160]".."ВАСИЛИЙ:".."\\n".."%c[255,255,128,128]Меченый, холодное оружие готово. Можешь забирать.", nil, nil, 30000, nil, [[sms\vasiliy\xo_repair_finished]])
		give_info("xo_repair_finished")
		amk.del_variable("xo_rep")
	end
end

function xo_repair_done()
	got(amk.load_variable("xo_sec", ""), 1)
	amk.del_variable("xo_sec")
end

