-- Пути к озвучкам
local senders = {
	["nano"]	= [[volazar]],
	["ozersky"]	= [[ozersky]],
	["sidor"]	= [[sidor]],
	["strelok"]	= [[strelok]],
	["unknown"]	= [[volazar]],
	["vol"]		= [[volazar]],
	["vol_new"]	= [[volazar]],
}

-- Мой вариант вывода СМС
function tips(text, sender, showtime, ozvuchka)
    if sender == nil then
		sender = "vol_new"
	end

	sms(text, nil, sender, showtime, nil, [[sms\]]..senders[sender]..[[\]]..ozvuchka)
end
