-- Первый слот всех флешек
function cheat:cheat_t1()					-- Трек 1
	local prefix = player_ogg.work_flash_return()
	if prefix == nil then
		return
		
	end

	sound = Sounds[player_ogg.work_flash_return()]..[[1]]		--Определяю флешку в рабочем слоте
	player_ogg.s_play(sound,1)
	player_ogg.set_play_flag(1)
	play_menu = false	
	self:on_quit()
end

