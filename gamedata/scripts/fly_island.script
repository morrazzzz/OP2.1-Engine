-- скриптовый спавн острова
function create_fly_island(pos,lv,gv)
	local obj = create("fly_island",pos,lv,gv)
	netpacket:modify(obj, 
		"object_flags", -198,		-- 0xffffff3a object_flags
		"visual_flags", 1,			-- 0x1 visual_flags
		"skeleton_name", "idle",
		"skeleton_flags", 1,
		"physic_type", 3,			-- 0x3
		"mass", 100,
		"fixed_bones", "link")
	create("zone_fly_island",pos,lv,gv)
	return obj
end

-- Удаление островов с уровня после нахождения тайника и ухода с территории
function release_completed_fly_islands()
	if not has_info("snp_bibliofrend3_done") or has_info("fli_done") then return end -- задание еще не взято или уже выполнено
	
	local lvl
	
	-- есть ли что удалять?
	for i = 1,#levels do
		lvl = tostring(levels[i])
		
		if has_info("fli_have"..lvl) and not has_info("fli_done"..lvl) and ai:level_id() ~= levels[i] then
			-- актор ушел с территории и есть что удалять - удаляем
			give_info("fli_done"..lvl)
			release_fly_islands_from_location(levels[i])
			return
		end
	end
end

-- собственно удаление островов с территории
function release_fly_islands_from_location(lvl)
	local obj, section

	for i=1,65534 do
		obj = server_object(i)
		if obj then
			section = obj:section_name()
			
			if (section == "fly_island" or section == "zone_fly_island" or section == "zone_tuman3") and
				object_level(obj) == lvl
			then
				release(obj, true)
			end
		end
	end
end

