-- Выброс на остров, тогда на землю не падают
function drop_code()
	for i = 1,#items do
		db.actor:drop_item_and_teleport(level_object(items[i]), db.actor:position():add(random_dir()))
	end
	items = {}
end

function random_dir()
	local a = math.random()*math.pi*2
	local r = math.random()*2 -- радиус разброса
	local x = math.cos(a)*r
	local z = math.sin(a)*r
	return vector():set(x, 1, z)
end

