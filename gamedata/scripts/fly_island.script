-- Обработчик. Передается секция взятого объекта. start = true - запуск новой цепочки.
function final_worker(section, start)
	local obj = finals[section]
	
	-- удаляем предыдущий объект
	if obj then
		-- удаляем телепорт
		if config:line_exist(section, "teleport") then
			kostya_dialog.delete_teleport(section)
		end
		
		-- удаляем записку
		if obj.note then
			amk.remove_item_from_inventory_by_name(obj.note, db.actor)
		end

		-- выдаем поршень
		if obj.info then
			give_info(obj.info)
		end

		-- выполняем функцию
		if obj.func then
			loadstring(obj.func)()
		end
	end
	
	if not start then
		section = finals[section].next_section
	end
	if not section then return end
	obj = finals[section]

	-- создаем следующий объект
	if obj then
		if config:line_exist(section, "teleport") then
			kostya_dialog.create_teleport(section, xyz(obj.position.x, obj.position.y, obj.position.z), obj.lv, obj.gv)
		else
			create(section, xyz(obj.position.x, obj.position.y, obj.position.z), obj.lv, obj.gv)
		end
	end
end

