function new_hit(obj, who, wpn, ammo, bone_index, bone_name, amount, hit_type)
	if not who or who:id()~=db.actor:id() then return end
	if not has_info("mera_kogot_have") or has_info("mera_kogot_done") then return end
		if string.find(obj:name(),"hlist_zombied") then
			local value = amk.load_variable("hlist_headshot", 0)+1
			amk.save_variable("hlist_headshot", value)
			if (bone_name=="bip01_head" or bone_name=="eye_left" or bone_name=="eye_right" or bone_name=="eyelid_1" or bone_name=="jaw_1" or bone_name=="bip01_neck") and not obj:alive() then
				if value == 1 then
					give_info("mera_kogot_headshot")
					amk.del_variable("hlist_headshot")
				end
			end
		end
end

