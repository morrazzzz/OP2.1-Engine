function add_docs_spot()
	for i=1,65534 do
		obj = server_object(i)

		if obj and string.find(obj:section_name(), "zonedocs") then
			level.map_add_object_spot_ser(obj.id, "profi", "Документ Воронина")
			db.god.c[5] = db.god.c[5]+1
		end
	end
end

function remove_docs_spot()
	for i=1,65534 do
		obj = server_object(i)

		if obj and string.find(obj:section_name(), "zonedocs") and level.map_has_object_spot(obj.id, "profi") ~= 0 then
			level.map_remove_object_spot(obj.id, "profi")
		end
	end
end

