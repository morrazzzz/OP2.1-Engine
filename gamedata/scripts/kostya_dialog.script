function checking_taken_item61(obj)
	if string.find(obj:section(),"zonedocs") then
		give_info("zonedocs_have"..string.sub(obj:section(),string.find(obj:section(),"_")+1))

		local total = 60
		if     has_info("voronin_docs1_start_as") then
			total = 24
		elseif has_info("voronin_docs1_start_yan") then
			total = 23
		elseif has_info("voronin_docs1_start_dt") then
			total = 22
		elseif has_info("voronin_docs1_start_atp") then
			total = 15
		elseif has_info("voronin_docs1_start_td") then
			total = 30
		elseif has_info("voronin_docs1_start_puzir") then
			total = 6
		elseif has_info("voronin_docs1_start_agro") then
			total = 26
		elseif has_info("voronin_docs1_start_marsh") then
			total = 30
		elseif has_info("voronin_docs1_start_esc") then
			total = 60
		elseif has_info("voronin_docs1_start_garbage") then
			total = 60
		elseif has_info("voronin_docs1_start_bar") then
			total = 60
		end
		
		if     has("zone_docs",total-1) then
			give_info("voronin_docs1_have")
		elseif has("zone_docs",total-3) then
			give_info("voronin_docs1_almost_have")
		end
		
		amk.remove_item_from_inventory(obj,db.actor)
		-- удаляем дубликаты
		if  (obj:section()=="zonedocs_26" and level.name() == "l03u_agr_underground") or 
			(obj:section()=="zonedocs_22" and (level.name() == "l06_rostok" or level.name() == "l05_bar"))
		then
			local sobj
			for i = 1,65534 do
				sobj = server_object(i)
				if sobj then
					if  (sobj:section_name() == "zonedocs_26" and level.name() == "l03u_agr_underground") or 
						(sobj:section_name() == "zonedocs_22" and (level.name() == "l06_rostok" or level.name() == "l05_bar"))
					then
						release(sobj,true)
						break
					end
				end
			end
		end
		
		create("zone_docs",xyz(0,0,0),0,0,0)
	end
end

