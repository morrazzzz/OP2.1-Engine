function spawn_teleports_pripyat()
  local se_obj = create("chess_teleport_1", xyz(9.3173294067383, 1.1479508876801, 100.24196624756), 117924 , 2142)
  xr_logic.pstor_store(db.actor, "chess_teleport_1", se_obj.id)
