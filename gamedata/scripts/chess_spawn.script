function check_notes(n)
  if dont_has_info("info_note_zone_"..n) then
    give_info("info_note_zone_"..n)
    if n <= #notes_array then
      local d = notes_array[n]
      create("note_zone_"..(n+1), xyz(d.position.x, d.position.y, d.position.z), d.lvid , d.gvid)
    end
  end
end

