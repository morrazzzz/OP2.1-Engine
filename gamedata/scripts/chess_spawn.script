-- Û‰‡ÎˇÂÏ ÚÂÎÂÔÓÚ˚
function delete_teleports_pripyat()
  local obj_id, se_obj, i
  for i = 1, 4 do
    obj_id = xr_logic.pstor_retrieve(db.actor, "chess_teleport_"..i, -1, true)
    se_obj = server_object(obj_id)
    if se_obj then
      release(se_obj, true)
    end
  end
end