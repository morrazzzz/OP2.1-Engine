-- при получении записки в инвентарь спавним тайник
function on_item_take(obj)
	if obj:section()=="val_zapis_1" and not has_info("coll_tele1_on") then
		spawn_next(1)
	end
end

function spawn_next(n)
	-- удаляем старую записку
	amk.remove_item(db.actor:object("val_zapis_"..tostring(n-1)))
	db.god.u[4] = false
	ui_mm_opt_main.GodHudUpdate()

	if n <= #treasures then
		-- выбираем где спавнить
		local a = treasures[n]
		local b = a[math.random(#a)]
		create("val_taynik_"..n, xyz(b.position.x,b.position.y,b.position.z), b.lv, b.gv)
		
		-- Телепорт вместо увеличенного прыжка для тайника на Кордоне и другие дополнения
		if n==1 then
		  if not has_info("coll_tele1_on") then 
			local se_obj = create("val_teleport_1",xyz(346.98187255859,18.106950759888,-15.256704330444),587878,2)
			xr_logic.pstor_store(db.actor, "val_teleport_1", se_obj.id)	
			give_info("coll_tele1_on")
		  end
		
		elseif n==2 then
		  if has_info("coll_tele1_on") then 
			local obj_id = xr_logic.pstor_retrieve(db.actor, "val_teleport_1", -1, true)
			local se_obj = server_object(obj_id)
			if se_obj then
				release(se_obj, true)
			end
			disable_info("coll_tele1_on")
		  end

		elseif n==16 and b==2 then
			create("amk_kanistra",xyz(39.229396820068,-0.0083452463150024,150.32916259766),67621,1308)

		elseif n==54 then
			valerich_dialog.spawn_tel()
		end
	else
		-- последняя записка с холмов Радара - удаляем телепорты
		valerich_dialog.release_tel()
		meceniy_in_hell.effects()
		meceniy_in_hell.horror_sound()	
		spawn_coll()
		give_info("coll_end")
	end
end

function spawn_coll()
	local zombi_skeletons={
	"physics\\dead_body\\skelet_combine_pose_03",
	"physics\\dead_body\\skelet_combine_pose_04",
	"physics\\dead_body\\skelet_combine_pose_05"
	}

	amk_particle.amk_particle({
			particle="amk\\zomb_explode",
			pos=db.actor:position()
		})
		
	local skel = amk.spawn_item("breakable_object",db.actor:position(),db.actor:game_vertex_id(),db.actor:level_vertex_id())
	netpacket:modify(skel, "visual_name", table.random(zombi_skeletons))
	--[[
	local t = amk.get_breakable_data(skel)
	t.visual_name = zombi_skeletons[math.random(#zombi_skeletons)]
	t.mass = 1.0
	t.physic_type = 3
	t.skeleton_flags = 1
	amk.set_breakable_data(t,skel)
	--]]
end

function add_coll_spot()
	for i=1,65534 do
		obj = server_object(i)

		if obj and string.find(obj:section_name(), "val_taynik_") then
			level.map_add_object_spot_ser(obj.id, "treasure", "Тайник Коллекционера")
			db.god.c[4] = db.god.c[4]+1
			return
		end
	end
end

function remove_coll_spot()
	for i=1,65534 do
		obj = server_object(i)

		if obj and string.find(obj:section_name(), "val_taynik_") and level.map_has_object_spot(obj.id, "treasure") ~= 0 then
			level.map_remove_object_spot(obj.id, "treasure")
			return
		end
	end
end

	