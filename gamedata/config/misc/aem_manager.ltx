; stakes
; особый вид боёв, при котором одна из команд из списка <teams1>
; будет сражаться с одной из команд из списка <teams2>
; - в диалоге будет создано (teams1 * teams2) веток боёв
;title			= "aem_" .. name
;info			= "aem_" .. name .. "_info"
;done			= "aem_" .. name .. "_done"
;teams1			= *
;teams2			= *
;fights			= *

[stakes]
teams1			= team_pseudodog, team_boar, team_snork, team_bloodsucker, team_zombie, team_chimera, team_burer, team_gigant
teams2			= team_dolg, team_neutral,team_military, team_killer, team_freedom, team_bandit
fights			= stakes_1, stakes_2, stakes_3, stakes_4, stakes_5

; описание боя для stakes
;[name]
;done			= "aem_" .. name .. "_done"
;intro			= nil
;extro			= nil
;team1			= "aem_" .. name .. "_1"
;team2			= "aem_" .. name .. "_2"

