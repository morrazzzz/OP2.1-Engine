; turnament
; особый вид боёв, при котором 4 случайных команды
; будут последовательно биться за звание чемпионов
; - в диалоге будет создано (teams * (teams-1)) веток боёв
;info			= "aem_" .. name .. "_info"
;done			= "aem_" .. name .. "_done"
;teams			= *
;fights			= *

[turnament]
